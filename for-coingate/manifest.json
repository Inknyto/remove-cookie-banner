{
  "manifest_version": 2,
  "name": "Remove Stackoverflow-like websites cookie banner",
  "version": "1.0",
  "description": "Logs the current URL to the console on page load.",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background-script.js"],
    "persistent": false
  },
  "content_scripts": [
    {
     "matches": [

     			 "https://coingate.com/*"
			 ],

      "js": ["content-script.js"]
    }
  ]
}
